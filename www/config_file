
server{
	server_name localhost;
	listen 8004;

	autoindex on;


	client_max_body_size 1000000000;

	accepted_methods	GET POST;

	location / {
		cgi .php /usr/bin/php-cgi;
		index cookie.php;
		root /sgoinfre/goinfre/Perso/dodjian/webserv/www;
		accepted_methods	GET POST DELETE;
	}

}

