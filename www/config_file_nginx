user www-data;

events {}
http{
server{
	server_name hello3 hello1 ;
	listen 777;

	autoindex off;
	root /home/user42/Bureau/webserv/www;
	dav_methods DELETE;
	index cookie.php;

	location ~ \.php$ {
		fastcgi_pass unix:/usr/bin/python2.7;
		fastcgi_index hello.php;
		fastcgi_param SCRIPT_FILENAME $document_root$fastcgi_script_name;
		include /etc/nginx/fastcgi_params;
	}
}}
