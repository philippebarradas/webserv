
server {
	server_name hello1 salut 123 pli;
	server_name hello2;
	server_name hello3;
	server_name hello4;
	listen 8001;

	client_max_body_size	20;

	cgi .php php-cgi;
	cgi .py cgi_python;

	accepted_methods	GET POST DELETE;

	root /home/user42/Bureau/webserv/srcs/Config/default/html_page;
	index my_index.html;
	autoindex off;
	error_page 400 /400_bad_request.html;
	error_page 404 /404_not_found.html;
	error_page 405 /405_not_allowed.html;

	location / {
		accepted_methods GET DELETE;
		index form.html;
		root /home/user42/Bureau/webserv/srcs/Config/default/html_page;
		error_page 400 /error_page/400.html;
		error_page 404 /error_page/404.html;
	}
}

