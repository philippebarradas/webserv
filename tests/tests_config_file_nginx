events {}

http{
server {
	listen 8006;

	root /home/user42/Bureau/webserv/tests;
	location / {
	}

	location /post_body {
		client_max_body_size	100;
	}
	location /directory/ {
		error_page 404 youpi.bad_extension;
		autoindex on;
		root /home/user42/Bureau/webserv/tests/YoupiBanane;
		index youpi.bad_extension;
	}

	location ~ \.bla$ {
		fastcgi_pass unix:/home/user42/Bureau/webserv/ubuntu_cgi_tester;
		fastcgi_param SCRIPT_FILENAME $document_root$fastcgi_script_name;
		include /etc/nginx/fastcgi_params;
	}
}}
